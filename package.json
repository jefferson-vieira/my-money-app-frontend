{
  "name": "my-money-app-frontend",
  "version": "0.1.0",
  "private": true,
  "description": "É uma aplicação desenvolvida para ajudar a gerênciar multiplas contas bancárias e a situação financeira dos nossos usuários",
  "keywords": [
    "management",
    "money",
    "personal"
  ],
  "homepage": "https://meu-dinheiro-frontend.herokuapp.com",
  "bugs": {
    "url": "https://github.com/jefferson-vieira/my-money-app-frontend/issues",
    "email": "jefferson98.vieira@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jefferson-vieira/my-money-app-frontend.git"
  },
  "author": {
    "name": "Jefferson Vieira da Silva",
    "email": "jefferson98.vieira@gmail.com",
    "url": "https://github.com/jefferson-vieira"
  },
  "main": "src/index.js",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "react-scripts build --colors",
    "lint": "eslint --ext .js,.jsx src",
    "start": "react-scripts start --colors",
    "start:prod": "serve -s build",
    "test": "react-scripts test --colors",
    "test:coverage": "react-scripts test --colors --coverage",
    "test:watch": "react-scripts test --colors --watch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "date-fns": "^1.30.1",
    "node-sass": "^4.11.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-redux-toastr": "^7.4.7",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "react-tooltip": "^3.10.0",
    "react-transition-group": "^4.1.0",
    "redux": "^4.0.1",
    "redux-form": "^8.2.3",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "reduxsauce": "^1.0.1",
    "sweetalert2": "^8.11.7",
    "sweetalert2-react-content": "^1.0.3"
  },
  "devDependencies": {
    "eslint-plugin-prettier": "^3.1.0",
    "harmony-reflect": "^1.6.1",
    "nock": "^10.0.6",
    "prettier": "^1.18.2",
    "serve": "^11.0.2",
    "source-map-explorer": "^2.0.1"
  },
  "engines": {
    "node": ">= 10.16.0",
    "npm": ">= 6.9.0",
    "yarn": "^1.16.0"
  }
}
