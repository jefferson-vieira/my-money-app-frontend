{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./",
    "jsx": "react",
    "paths": {
      "assets/*": ["src/assets/*"],
      "components/*": ["src/components/*"],
      "services/*": ["src/services/*"],
      "configs/*": ["src/configs/*"],
      "pages/*": ["src/pages/*"],
      "routes/*": ["src/routes/*"],
      "store/*": ["src/store/*"],
      "utils/*": ["src/utils/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
